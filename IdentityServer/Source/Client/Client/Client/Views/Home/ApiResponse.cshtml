@model ApiResponseViewModel

@{
    ViewData["Title"] = "Api";
}

<h1>ApiResponse</h1>

<style>
    table, th, td {
        border: 1px solid black;
        padding: 0 5px;
    }
</style>

<table>
    <thead>
        <tr>
            <th>key</th>
            <th>value</th>
        </tr>
    </thead>


    @foreach (var item in Model.ArrayValues.Children<Newtonsoft.Json.Linq.JObject>())
    {
        @foreach (var prop in item.Properties())
        {
            <tr>
                <td>
                    @prop.Name
                </td>
                <td>
                    @prop.Value
                </td>
            </tr>
        }
    }


</table>
